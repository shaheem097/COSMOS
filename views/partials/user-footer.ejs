<script src="https://unpkg.com/sweetalert/dist/sweetalert.min.js"></script>
<script>
  // Get the logout link
  const logoutLink = document.getElementById('logout-link');

  // Add click event listener to the logout link
  logoutLink.addEventListener('click', (event) => {
    event.preventDefault();

    // Show sweet alert confirmation dialog
    swal({
      title: "Are you sure?",
      text: "You are about to logout.",
      icon: "warning",
      buttons: ["Cancel", "Logout"],
      dangerMode: true,
    })
      .then((willLogout) => {
        if (willLogout) {
          // If user clicks on "Logout" button in sweet alert dialog, redirect to logout URL
          window.location.href = "/logout";
        }
      });
  });
</script>
<script>
  document.getElementById("phoneInput").addEventListener("input", function () {
    var phoneInput = document.getElementById("phoneInput");
    var phoneError = document.getElementById("phoneError");
    var phonePattern = /^[0-9]{10}$/;
    var phone = phoneInput.value;
    if (!phone.match(phonePattern)) {
      phoneError.textContent = "Please enter a 10 digit phone number";
      phoneInput.setCustomValidity("Please enter a 10 digit phone number");
    } else {
      phoneError.textContent = "";
      phoneInput.setCustomValidity("");
    }
  });
</script>
<script>
  // JavaScript code
  function toggleVisibility() {
    var otpInput = document.getElementById("otp");
    var toggleBtn = document.getElementById("toggleBtn");

    if (otpInput.type === "password") {
      otpInput.type = "text";
      toggleBtn.innerHTML = "&#128064;"; // Update button icon to hide
    } else {
      otpInput.type = "password";
      toggleBtn.innerHTML = "&#128065;"; // Update button icon to unhide
    }
  }

</script>

<script>
  function runSearch() {
    document.getElementById('searchBtn').click()
  }


  function search_product() {
    let key = document.getElementById('searchbar').value
    if (key.length == 0) {
      document.getElementById('list').style.display = "none"
      document.getElementById('no').style.display = "none"
    }
    else {
      document.getElementById('list').style.display = "block"
    }

    let input = document.getElementById('searchbar').value
    input = input.toLowerCase()
    let x = document.getElementsByClassName('products')
    let counter = 0
    for (let i = 0; i < x.length; i++) {
      if (!x[i].innerHTML.toLowerCase().includes(input)) {
        x[i].style.display = "none"
        document.getElementById('no').style.display = "block"
      }
      else {
        counter++
        document.getElementById('no').style.display = "none"
        x[i].style.display = "list-item"
        if (counter == 4) {
          break;
        }
      }


    }
  }

</script>



